find_packages <- function() {
files <- list.files(pattern = "\\.Rmd?$", recursive = TRUE)
all_lines <- unlist(lapply(files, readLines))
# Match library() and require()
pkg_lines <- grep("library\\(|require\\(", all_lines, value = TRUE)
# Extract package names
pkgs <- gsub(".*library\\((['\"]?)([^)'\"]+).*", "\\2", pkg_lines)
pkgs <- gsub(".*require\\((['\"]?)([^)'\"]+).*", "\\2", pkgs)
unique(pkgs[pkgs != pkg_lines])  # Filter out failed matches
}
